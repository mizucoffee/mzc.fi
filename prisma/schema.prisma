// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id              String    @id
  sid             String    @unique
  data            String
  expiresAt       DateTime
}

model User {
  id              Int       @id   @default(autoincrement())
  name            String    @unique
  password_digest String
}
